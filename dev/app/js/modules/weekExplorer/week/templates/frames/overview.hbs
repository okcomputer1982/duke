<div class="headingContainer">
  <h1 class="title">overview</h1>
  <h1 class="subtitle">{{{content.title}}}</h1>
</div> 

<div class="inner-wrap attributeTarget">
{{#if content.image}}
  <div class="row">
    <div class="col-md-4">
      <img class="sideImage" src="../images/frame_images/{{content.image}}" alt="">
    </div>
    <div class="col-md-8">
{{/if}}
      <h2 class="heading">Overview:</h2>
      {{#if content.overview}}
        <div class="textblock">
          {{{content.overview}}}
        </div>
      {{/if}}

      <h2 class="heading">You Will:</h2>
        <div class="textblock">
          <ul class="objectives">
            {{#each content.objectives}}
              <li>{{{this}}}</li>
            {{/each}}
          </ul>

        </div>

        <div class="textblock">
          <p><strong>When registering for sites linked from this course, please use the same username and password you were given for A Student's Journey when creating your account.</strong></p>
        </div>
{{#if image}}
    </div>
  </div>
{{/if}}
</div>